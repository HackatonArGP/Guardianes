            <Border x:Name="Image1Border" Opacity="0" BorderThickness="0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="110" Height="100" Margin="0,100,0,0">
                <Image x:Name="Image1" Stretch="None" HorizontalAlignment="Center" VerticalAlignment="Center" />
            </Border>
            <Border x:Name="Image2Border" Opacity="0" BorderThickness="0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="110" Height="100" Margin="80,150,0,0">
