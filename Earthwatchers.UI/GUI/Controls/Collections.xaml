    <Grid x:Name="LayoutRoot">
        <Grid.Background>
            <ImageBrush x:Name="BackgroundImage" ImageFailed="BackgroundImage_ImageFailed" Stretch="None" AlignmentX="Left" AlignmentY="Top" Opacity="0.3" />
