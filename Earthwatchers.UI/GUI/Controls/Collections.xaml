            <TextBlock x:Name="TitleText" Foreground="#FF333333" Opacity="0" FontSize="20" TextAlignment="Center" HorizontalAlignment="Center" FontFamily="Arial" FontWeight="Bold" IsHitTestVisible="False" Text="{Binding Path=CONGRATS, Source={StaticResource Labels}}">
          <LineBreak />
                <Run Text="{Binding Path=Collections2, Source={StaticResource Labels}}" />
            </TextBlock>

            <TextBlock x:Name="PrizeText" Opacity="0" Foreground="#FF9cae2d" FontSize="20" Margin="0,60,0,0" TextAlignment="Center" HorizontalAlignment="Center" FontFamily="Arial" FontWeight="Bold" IsHitTestVisible="False" />
        </Grid>
        <Grid x:Name="FooterGrid" Grid.Row="1" Height="65" Opacity="1">
            <Grid.Background>
                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                    <GradientStop Color="#FFe4e4e4" Offset="0"></GradientStop>
                    <GradientStop Color="#FFcbcdcc" Offset="1"></GradientStop>
                </LinearGradientBrush>
            </Grid.Background>
            <TextBlock x:Name="FooterText" Opacity="0" Foreground="#FF333333" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Arial" FontWeight="Bold">
            <Run Text="{Binding Path=Collections1, Source={StaticResource Labels}}" />
            <LineBreak /><Run x:Name="CollectionNameText" />
            </TextBlock>
            <Button x:Name="ContinueButton" Visibility="Collapsed" HorizontalAlignment="Right" Style="{StaticResource GreenButton}" Width="140" Height="35" Foreground="White" FontSize="15" FontWeight="Bold" Content="{Binding TutorialGame1, Source={StaticResource Labels}}" Margin="0,13,23,17" Click="ContinueButton_Click" />
        </Grid>

        <Grid x:Name="CompleteCollectionGrid" IsHitTestVisible="False" Visibility="Collapsed" Opacity="0">
            <Grid.Background>
                <SolidColorBrush Color="Black" Opacity="0.9" />
            </Grid.Background>
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition Height="Auto" />
                <RowDefinition />
            </Grid.RowDefinitions>
            <TextBlock x:Name="CompleteText1" Opacity="0" Foreground="#FFFFFFFF" FontSize="20" TextAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,20" HorizontalAlignment="Center" FontFamily="Arial" FontWeight="Bold" IsHitTestVisible="False" Text="{Binding Path=Collections3, Source={StaticResource Labels}}">

            </TextBlock>

            <TextBlock x:Name="CollectionCompleteText" Opacity="0" Foreground="#FF9cae2d" Text="{Binding Path=Collections2, Source={StaticResource Labels}}" Grid.Row="1" FontSize="20" VerticalAlignment="Center" TextAlignment="Center" HorizontalAlignment="Center" FontFamily="Arial" FontWeight="Bold" IsHitTestVisible="False" />

            <TextBlock x:Name="CompleteText2" Opacity="0" Foreground="#FFFFFFFF" Grid.Row="2" FontSize="16" VerticalAlignment="Center" TextAlignment="Center" HorizontalAlignment="Center" FontFamily="Arial" IsHitTestVisible="False" TextWrapping="Wrap" Text="{Binding Path=Collections4, Source={StaticResource Labels}}">
                <LineBreak />
            </TextBlock>
            <TextBlock Text="{Binding Path=Collections5, Source={StaticResource Labels}}">

            </TextBlock>
        </Grid>

        <Button x:Name="CloseButton" Opacity="0" Grid.Row="1" Visibility="Collapsed" HorizontalAlignment="Right" Style="{StaticResource GreenButton}" Width="140" Height="35" Foreground="White" FontSize="15" FontWeight="Bold" Content="{Binding Path=HexInfo4, Source={StaticResource Labels}}" Margin="0,0,20,0" Click="CloseButton_Click" />

        <MediaElement x:Name="foundSound" AutoPlay="False" MediaFailed="sound_MediaFailed" Source="../../Resources/tada.mp3" />
