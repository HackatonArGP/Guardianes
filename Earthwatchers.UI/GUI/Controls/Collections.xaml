            <Polygon x:Name="PolygonExtra2" Opacity="0" Points="0,50 30,0 80,0 110,50 80,100 30,100" Stroke="#FFe4e4e4" Fill="#FFf9f9f9" Margin="80,50,0,0" StrokeThickness="2" VerticalAlignment="Top" HorizontalAlignment="Left" />
            <Polygon x:Name="PolygonExtra3" Opacity="0" Points="0,50 30,0 80,0 110,50 80,100 30,100" Stroke="#FFe4e4e4" Fill="#FFf9f9f9" Margin="160,0,0,0" StrokeThickness="2" VerticalAlignment="Top" HorizontalAlignment="Left" />
            <Polygon x:Name="PolygonExtra4" Opacity="0" Points="0,50 30,0 80,0 110,50 80,100 30,100" Stroke="#FFe4e4e4" Fill="#FFf9f9f9" Margin="240,50,0,0" StrokeThickness="2" VerticalAlignment="Top" HorizontalAlignment="Left" />
            <Polygon x:Name="PolygonExtra5" Opacity="0" Points="0,50 30,0 80,0 110,50 80,100 30,100" Stroke="#FFe4e4e4" Fill="#FFf9f9f9" Margin="320,0,0,0" StrokeThickness="2" VerticalAlignment="Top" HorizontalAlignment="Left" />


            <Polygon x:Name="Polygon1" RenderTransformOrigin="0.5,0.5" Opacity="0" Points="0,50 30,0 80,0 110,50 80,100 30,100" Stroke="#FFe4e4e4" Fill="#FFf9f9f9" Margin="0,100,0,0" StrokeThickness="2" VerticalAlignment="Top" HorizontalAlignment="Left">
                <Polygon.Effect>
                    <DropShadowEffect BlurRadius="15" Color="Transparent" ShadowDepth="0" x:Name="Polygon1ShadowEffect" />
                </Polygon.Effect>
                <Polygon.RenderTransform>
                    <ScaleTransform ScaleX="1" ScaleY="1" />
                </Polygon.RenderTransform>
            </Polygon>
            <Polygon x:Name="Polygon2" RenderTransformOrigin="0.5,0.5" Opacity="0" Points="0,50 30,0 80,0 110,50 80,100 30,100" Stroke="#FFe4e4e4" Fill="#FFf9f9f9" Margin="80,150,0,0" StrokeThickness="2" VerticalAlignment="Top" HorizontalAlignment="Left">
                <Polygon.Effect>
