        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid x:Name="CollectionsTutorial" Grid.Row="0" Margin="35,20,35,0" Visibility="Collapsed">
            <TextBlock Grid.Row="0" Foreground="Black" Text="{Binding Path=CollectionsTutorial, Source={StaticResource Labels}}" TextWrapping="Wrap" />
            <Grid />
        </Grid>
        <Grid x:Name="MainGrid" Grid.Row="0" Margin="35,20,35,0" Opacity="1">
