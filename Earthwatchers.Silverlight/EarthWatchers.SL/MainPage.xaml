    <Grid x:Name="mainGrid" Background="Black">
        <Grid>
            <Grid x:Name="mapGrid" RenderTransformOrigin="0.5,0.5" Background="#FFEFEFEF">
                <Grid.RenderTransform>
                    <CompositeTransform/>
                </Grid.RenderTransform>

                <Grid x:Name="mapWrapper">
                    <Grid x:Name="Tutorial0" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="200,220,0,0" Canvas.ZIndex="2">
                        <Grid.RenderTransform>
                            <ScaleTransform ScaleX="0" ScaleY="0" CenterX="100" CenterY="30" />
                        </Grid.RenderTransform>
                        <Border CornerRadius="8" BorderThickness="1" BorderBrush="#FF7f7f7f" Width="245" Height="125" Margin="10,0,0,0">
                            <Border.Background>
                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                    <GradientStop Color="White" Offset="0" />
                                    <GradientStop Color="#FFdddddd" Offset="1" />
                                </LinearGradientBrush>
                            </Border.Background>
                            <StackPanel Orientation="Vertical">
                                <TextBlock TextWrapping="Wrap" VerticalAlignment="Center" Foreground="#FF333333" FontFamily="/EarthWatchers.SL;component/Resources/MyriadPro-Regular.otf#Myriad Pro" FontSize="14" Margin="6">
                            Este es el mapa de la zona de riesgo. 
                            <LineBreak /><Run FontWeight="Bold">¡De vos depende proteger esta zona!</Run>
                            <LineBreak />Podés moverte en él haciendo click y moviendo el mouse.
                                </TextBlock>
                                <Button Style="{StaticResource GreenButton}" Width="140" Height="30" Foreground="White" FontSize="15" FontWeight="Bold" Content="CONTINUAR >>" HorizontalAlignment="Right" Margin="0,5,10,0" Click="Step0Click" />
                            </StackPanel>
                        </Border>
                    </Grid>

                    <Grid x:Name="Tutorial1" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="200,140,0,0" Canvas.ZIndex="2">
                        <Grid.RenderTransform>
                            <ScaleTransform ScaleX="0" ScaleY="0" CenterX="100" CenterY="30" />
                        </Grid.RenderTransform>
                        <Border CornerRadius="8" BorderThickness="1" BorderBrush="#FF7f7f7f" Width="215" Height="105" Margin="10,0,0,0">
                            <Border.Background>
                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                    <GradientStop Color="White" Offset="0" />
                                    <GradientStop Color="#FFdddddd" Offset="1" />
                                </LinearGradientBrush>
                            </Border.Background>
                            <StackPanel Orientation="Vertical">
                                <TextBlock TextWrapping="Wrap" VerticalAlignment="Center" Foreground="#FF333333" FontFamily="/EarthWatchers.SL;component/Resources/MyriadPro-Regular.otf#Myriad Pro" FontSize="14" Margin="6">
